{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StringFilterOptions": {
    "MinLength": 10
  },

  "ExpressionBalanceOptions": {
    "CheckParentheses": true,
    "CheckBrackets": true,
    "CheckBraces": true
  },

  "HotPotatoGameOptions": {
    "MaxRandomNumber": 100
  },

  "WordCounterOptions": {
    "IgnoreCase": true
  }
}
